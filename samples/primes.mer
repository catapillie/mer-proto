func is_prime(n)
{
    if n <= 1 then
        return false

    if n == 2 then
        return true

    var i = 2
    while i < n do
    {
        if n % i == 0 then
            return false

            i = i + 1
    }

    return true
}

var i = 1
while i <= 100 do
{
    i
    is_prime(i)
    i = i + 1
}
