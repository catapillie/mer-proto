var n = 100
var arr = alloc<[3]f64>(n)
var p = &arr

var b = true

var i = 0
var f = 0.0
while i < n do
{
    (@p)[i].0 = f
    (@p)[i].1 = f * f
    (@p)[i].2 = f * f * f

    b = b & ((@p)[i].0 == f)
    b = b & ((@p)[i].1 == f * f)
    b = b & ((@p)[i].2 == f * f * f)

    i = i + 1
    f = f + 0.25
}

var i = 0
var f = 0.0
while i < n do
{
    (@p)[i] = [f, f * f, f * f * f]
    b = b & ((@p)[i].0 == f)
    b = b & ((@p)[i].1 == f * f)
    b = b & ((@p)[i].2 == f * f * f)

    i = i + 1
    f = f + 0.25
}

return b